{
  "name": "Loudmouth",
  "version": "1.0.0",
  "description": "An application for chating with friends with Facebook authentication. Developed for SDIS @ FEUP.",
  "main": "main.js",
  "author": {
    "name": "LGP-1D"
  },
  "scripts": {
    "start": "electron --debug .",
    "watch": "webpack -d --watch",
    "webpack-test-build": "webpack -d --config webpack-test.config.js",
    "test": "jest ./tests/unit --coverage",
    "test-integration": "electron-mocha ./tests/integration",
    "prerelease": "webpack -p",
    "release": "build --windows --x64"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.28.0",
    "electron": "^1.6.2",
    "electron-builder": "^16.8.2",
    "electron-mocha": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "ignore-loader": "^0.1.2",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "react-svg-pathline": "^0.3.0",
    "react-test-renderer": "^15.4.2",
    "sass-loader": "^6.0.3",
    "spectron": "^3.6.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "electron-reload": "^1.1.0",
    "immutable": "^3.8.1",
    "jsplumb": "^2.3.2",
    "oauth": "^0.9.15",
    "prop-types": "^15.5.6",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-designer": "^1.0.8",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.12.2",
    "react-icons": "^2.2.3",
    "react-redux": "^5.0.3",
    "react-redux-toastr": "^5.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "redux": "^3.6.0",
    "superagent": "^3.5.2",
    "uuid": "^3.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "build": {
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "release"
    }
  }
}
